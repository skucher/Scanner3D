#import "SculpImage.h"


@implementation SculpImage
@synthesize sculpted;

-(id)initWithCGImage:(CGImageRef)cgi :(SculptData) _sculptData
{
    self = [super initWithCGImage:cgi];
    if(self)
    {
        sculptData = _sculptData;
        sculpted = NO;
    }
    return self;
}

@end

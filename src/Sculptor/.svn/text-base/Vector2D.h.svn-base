#pragma once

/**Vector2D is vector 2D representation*/
class Vector2D
{
public:
	/**parameterless constructor*/
	Vector2D(void);
	/**constructor:
	*@param x: x location
	*@param y: y location
	*/
	Vector2D(float x,float y);
	~Vector2D(void);

	float getX()
	{
		return _x;
	}
	
	float getY()
	{
		return _y;
	}

protected:
	float _x;
	float _y;
};

